<!-- BEGIN LOGIN -->
<div class="content">
    <!-- BEGIN LOGIN FORM -->

    <form class="login-form" action="{{ path("fos_user_security_check") }}" method="post">
        <input type="hidden" name="_csrf_token" value="{{ csrf_token }}"/>
        <h3 class="form-title">Iniciar sesión</h3>
        <div class="alert alert-danger display-hide">
            <button class="close" data-close="alert"></button>
			<span>
			Entre un usuario y contraseña válidas. </span>
        </div>
        {% if error %}
            <div class="alert alert-danger">
                <button class="close" data-close="alert"></button>
			<span>
			Credenciales inválidas. </span>
            </div>
        {% endif %}
        <div class="form-group">
            <!--ie8, ie9 does not support html5 placeholder, so we just show field title for that-->
            <label class="control-label visible-ie8 visible-ie9">Dirección de correo</label>

            <div class="input-icon">
                <i class="fa fa-envelope"></i>
                <input class="form-control placeholder-no-fix" type="text" autocomplete="off"
                       placeholder="Dirección de correo" id="username" name="_username" required="required" value="{{ last_username }}"/>
            </div>
        </div>
        <div class="form-group">
            <label class="control-label visible-ie8 visible-ie9">Password</label>

            <div class="input-icon">
                <i class="fa fa-lock"></i>
                <input class="form-control placeholder-no-fix" type="password" autocomplete="off"
                       placeholder="Contraseña" id="password" name="_password" required="required"/>
            </div>
        </div>
        <div class="form-actions">
            {#<label class="checkbox">
                <input type="checkbox" id="remember_me" name="_remember_me" value="on"/> Recordarme </label>#}
            <input type="submit" id="_submit" name="_submit" class="btn green pull-right" value="Iniciar"/> <i class="m-icon-swapright m-icon-white"></i>
        </div>
        <div class="forget-password">
            <h4>¿Olvidaste tu contraseña?</h4>

            <p>
                No te preocupes, haz clic <a href="javascript:;" id="forget-password">
                    aquí </a> para reestablecer tu contraseña.
            </p>
        </div>
    </form>
    <!-- END LOGIN FORM -->
    <!-- BEGIN FORGOT PASSWORD FORM -->
    <form action="{{ path('fos_user_resetting_send_email') }}" method="POST" class="forget-form fos_user_resetting_request">
        <h3>¿Olvidaste tu contraseña?</h3>

        <p>
            Escriba su dirección de correo abajo para reestablecer su contraseña.
        </p>

        <div class="form-group">
            <div class="input-icon">
                <i class="fa fa-envelope"></i>
                <input class="form-control placeholder-no-fix" type="text" autocomplete="off"
                       placeholder="Dirección de correo" id="reset_username" name="reset_username"/>
            </div>
            {% if invalid_username is defined %}
                <span id="invalid_username" class="help-block">Dirección de correo inválida.</span>
            {% endif %}
        </div>
        <div class="form-actions">
            <button type="button" id="back-btn" class="btn">
                <i class="m-icon-swapleft"></i> Atrás
            </button>
            <button type="submit" class="btn green pull-right">
                Reestablecer <i class="m-icon-swapright m-icon-white"></i>
            </button>
        </div>
    </form>
    <!-- END FORGOT PASSWORD FORM -->
</div>
