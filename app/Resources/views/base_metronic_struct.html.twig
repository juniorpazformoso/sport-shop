            <span class="title">Catalogo</span>
            {% block rootmenucatalogoaux %} {% endblock %}
            <span class="arrow {% block rootmenucatalogoarrow %} {% endblock %}"></span>
        </a>
        <ul class="sub-menu">
            <li {% block rootmenucategory %} {% endblock %}>
                <a href="{{ path('category') }}">
                    <i class="icon-list"></i>
                    Categorías</a>
