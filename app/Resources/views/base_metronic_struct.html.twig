        </a>
    </li>
    
    <li {% block rootmenusnippets %} {% endblock %}>
        <a href="{{ path('snippet') }}">
            <i class="fa fa-file-text"></i>
            <span class="title">Textos</span>
            {% block rootmenusnippetsaux %} {% endblock %}
        </a>
    </li>
    
     
     <li {% block rootmenunotice %} {% endblock %}>
        <a href="javascript:;">
            <i class="fa fa-navicon"></i>
            <span class="title">Noticias</span>
            {% block rootmenunoticeaux %} {% endblock %}
            <span class="arrow {% block rootmenunoticearrow %} {% endblock %}"></span>
        </a>
        
        <ul class="sub-menu">
            <li {% block rootmenucategorynotice %} {% endblock %}>
                <a href="{{ path('notice') }}"><i class="icon-list"></i>Noticias</a>
            </li>
            
            <li {% block rootmenunoticecategories %} {% endblock %}>
                <a href="{{ path('categorynotice') }}"><i class="icon-list"></i>Categorías</a>
            </li>
        </ul>
     </li>
    
    
    
    <li {% block rootmenureports %} {% endblock %}>
        <a href="{{ path('report_invoice') }}">
