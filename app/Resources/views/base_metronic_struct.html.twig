{% extends '::base_metronic_layout.html.twig' %}

{% block page_title %}Dabber spain | Administración{% endblock %}

{% block stylesheet %}

{% endblock %}

{% block structure %}

    <!-- BEGIN HEADER -->
    <div class="page-header navbar navbar-fixed-top">
    <!-- BEGIN HEADER INNER -->
    <div class="page-header-inner">
    <!-- BEGIN LOGO -->
    <div class="page-logo">
        <a href="{{ path('service_backend_admin') }}" style="color: #FFFF00 !important;">
            <img src="{{ asset('web/bundles/metronic/jl/dabber.bmp') }}" alt="logo" width="60%" style="margin-top: 5px"/>
        </a>
        <div class="menu-toggler sidebar-toggler hide">
            <!-- DOC: Remove the above "hide" to enable the sidebar toggler button on header -->
        </div>
    </div>
    <!-- END LOGO -->
    <!-- BEGIN RESPONSIVE MENU TOGGLER -->
    <a href="javascript:;" class="menu-toggler responsive-toggler" data-toggle="collapse" data-target=".navbar-collapse">
    </a>
    <!-- END RESPONSIVE MENU TOGGLER -->
    <!-- BEGIN TOP NAVIGATION MENU -->
    <div class="top-menu">
    <ul class="nav navbar-nav pull-right">
    <!-- BEGIN USER LOGIN DROPDOWN -->
    <li class="dropdown dropdown-user">
        <a href="#" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-close-others="true">
            <img alt="" class="img-circle hide1" src="{{ asset('web/bundles/metronic/jl/avatar.png') }}"/>
                <span class="username username-hide-on-mobile">
                    {% if is_granted("IS_AUTHENTICATED_FULLY") %}
                        {{ app.user.email }}
                    {% else %}
                        Invitado
                    {% endif %}
                </span>
                {% if is_granted("IS_AUTHENTICATED_FULLY") %}
                    <i class="fa fa-angle-down"></i>
                {% endif %}
        </a>
        {% if is_granted("IS_AUTHENTICATED_FULLY") %}
            <ul class="dropdown-menu">
                <li>
                    <a href="{{ path('fos_user_profile_show') }}">
                        <i class="icon-user"></i> Perfil </a>
                </li>
                <li>
                    <a href="{{ path('fos_user_security_logout') }}">
                        <i class="icon-key"></i> Cerrar sesión </a>
                </li>
            </ul>
        {% endif %}
    </li>
    <!-- END USER LOGIN DROPDOWN -->
    </ul>
    </div>
    <!-- END TOP NAVIGATION MENU -->
    </div>
    <!-- END HEADER INNER -->
    </div>
    <!-- END HEADER -->

    <div class="clearfix">
    </div>

    <div class="page-container">
    <!-- BEGIN SIDEBAR -->
    <div class="page-sidebar-wrapper">
    <!-- DOC: Set data-auto-scroll="false" to disable the sidebar from auto scrolling/focusing -->
    <!-- DOC: Change data-auto-speed="200" to adjust the sub menu slide up/down speed -->
    <div class="page-sidebar navbar-collapse collapse">
    <!-- BEGIN SIDEBAR MENU -->
    <ul class="page-sidebar-menu" data-auto-scroll="true" data-slide-speed="200">
    <!-- DOC: To remove the sidebar toggler from the sidebar you just need to completely remove the below "sidebar-toggler-wrapper" LI element -->
    <li class="sidebar-toggler-wrapper" style="margin-bottom: 20px">
        <!-- BEGIN SIDEBAR TOGGLER BUTTON -->
        <div class="sidebar-toggler">
        </div>
        <!-- END SIDEBAR TOGGLER BUTTON -->
    </li>
    <li {% block rootmenuhome %} {% endblock %}>
        <a href="{{ path('service_backend_admin') }}">
            <i class="icon-home"></i>
            <span class="title">Inicio</span>
            {% block rootmenuhomeaux %} {% endblock %}
        </a>
    </li>
    
    <li {% block rootmenuorderclients %} {% endblock %}>
        <a href="{{ path('orderclient') }}">
            <i class="fa fa-compass"></i>
            <span class="title">Pedidos</span>
            {% block rootmenuorderclientsaux %} {% endblock %}
        </a>        
    </li>
    
    <li {% block rootmenuclient %} {% endblock %}>
        <a href="javascript:;">
            <i class="fa fa-user-md"></i>
            <span class="title">Clientes</span>
            {% block rootmenuclientaux %} {% endblock %}
            <span class="arrow {% block rootmenuclientarrow %} {% endblock %}"></span>
        </a>
        <ul class="sub-menu">
            <li {% block rootmenugender %} {% endblock %}>
                <a href="{{ path('gender') }}">
                    <i class="icon-list"></i>
                    Géneros</a>
            </li>
            
            <li {% block rootmenuclienti %} {% endblock %}>
                <a href="{{ path('client') }}">
                    <i class="icon-list"></i>
                    Clientes</a>
            </li>
            
            <li {% block rootmenuaddress %} {% endblock %}>
                <a href="{{ path('addressclient') }}">
                    <i class="icon-list"></i>
                    Direcciones</a>
            </li>
            
            
            
        </ul>
    </li>
    
    <li {% block rootmenuusers %} {% endblock %}>
        <a href="{{ path('admin_user',  { 'role': 'ROLE_ADMIN' }) }}">
            <i class="fa fa-group"></i>
            <span class="title">Usuarios</span>
            {% block rootmenuusersaux %} {% endblock %}
            {#<span class="arrow {% block rootmenuusersarrow %} {% endblock %}"></span>#}
        </a>        
    </li>
    
    <li {% block rootmenutemplateemail %} {% endblock %}>
